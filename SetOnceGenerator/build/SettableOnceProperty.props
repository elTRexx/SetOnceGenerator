<Project>
	<ItemGroup>
		<SettableNTimesPropertyToCopy Include="$(MSBuildThisFileDirectory)..\..\Copied_Files\**\*.*" />
	</ItemGroup>
	
	<Target Name="CopyPartialWarningsMethods" BeforeTargets="BeforeCompile">
		<Message Text="%(SettableNTimesPropertyToCopy.RecursiveDir)%(SettableNTimesPropertyToCopy.Filename)%(SettableNTimesPropertyToCopy.Extension)"/>
		<Copy SourceFiles="@(SettableNTimesPropertyToCopy)"			  
			  DestinationFolder="@(SettableNTimesPropertyToCopy -> '$(MSBuildProjectDirectory)\%(RecursiveDir)')"
			  Condition="!Exists('$(MSBuildProjectDirectory)\%(SettableNTimesPropertyToCopy.RecursiveDir)%(SettableNTimesPropertyToCopy.Filename)%(SettableNTimesPropertyToCopy.Extension)')"
			  />
	</Target>
</Project>